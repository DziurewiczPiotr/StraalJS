{
  "env": {
    "browser": true,
    "commonjs": true,
    "jasmine": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "indent": [1, 4, { "SwitchCase": 1 }],
    "linebreak-style": ["error", "unix"],
    "quotes": [2, "single"],
    "no-multi-spaces": 0,
    "no-console": 0,
    "brace-style": [2, "1tbs", { "allowSingleLine": true }]
  },
  "parserOptions": {
    "sourceType": "module"
  }
}
